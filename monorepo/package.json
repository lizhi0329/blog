{
  "name": "monorepo",
  "private": true,
  "license": "ISC",
  "scripts": {
    "clean:type": "rimraf ./dist",
    "type:src": "pnpm run clean:type && vue-tsc -p tsconfig.src.json --composite false --declaration --emitDeclarationOnly",
    "mv-type": "tsx ./scripts/dts-mv.ts",
    "build:uitls": "pnpm run type:src && pnpm --filter ./packages/utils run build && pnpm run mv-type",
    "script:test": "tsx ./scripts/test.ts",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue --fix ./"
  },
  "devDependencies": {
    "@mrx/utils": "workspace:^",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "child_process": "^1.0.2",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "rimraf": "^5.0.5",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.19"
  }
}
